{
  "compilerOptions": {
    "target": "es2021",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": false,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "module": "NodeNext",
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
